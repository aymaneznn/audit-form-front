{
  "name": "audit-form-lib",
  "version": "0.0.3",
  "files": [
    "dist/*",
    "*.ts",
    "*.json",
    "*.js"
  ],
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --coverage",
    "test:e2e": "vite build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "vite build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress run --browser chrome'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@vee-validate/i18n": "^4.12.4",
    "@vee-validate/rules": "^4.12.4",
    "axios": "^1.6.5",
    "chart.js": "^4.4.2",
    "date-fns": "^3.3.1",
    "keycloak-js": "^23.0.4",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "3.46.0",
    "vee-validate": "^4.12.4",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-istanbul": "^1.2.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "c8": "^9.1.0",
    "cypress": "^13.6.3",
    "cypress-keycloak": "^2.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^24.0.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "prettier": "^3.2.4",
    "sass": "^1.70.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "~5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.2.1",
    "vue-router-mock": "^1.0.0",
    "vue-tsc": "^1.8.27",
    "workbox-precaching": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
